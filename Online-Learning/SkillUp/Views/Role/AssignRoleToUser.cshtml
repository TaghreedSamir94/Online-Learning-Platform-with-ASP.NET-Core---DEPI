
@using SkillUp.ActionRequests.RoleRequest
@model AssignRoleToUserActionRequset

@{
    ViewData["Title"] = "Assign User To Role";
}
<form asp-action="AssignRoleToUser" method="post">
    <div class="form-group">
        <label asp-for="UserName">User</label>
        <select asp-for="UserName" class="from-control"></select>
    </div><div class="form-group">
        <label asp-for="RoleName">role</label>
        <select asp-for="RoleName" class="from-control"></select>
    </div>

    <button type="submit" class="btn btn-success">Assign Role </button>
</form>
@if (TempData["SuccessMassage"] != null)
{
    <div class="alert alert-success">
        @TempData["SuccessMassage"]
    </div>
}
@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
