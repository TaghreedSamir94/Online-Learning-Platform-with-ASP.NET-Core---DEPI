@using Microsoft.AspNetCore.Identity

@model IEnumerable<IdentityRole>

<h2>Role List</h2>
<table class="table">
    <thead>
        <tr>
            <th>Role name</th>
        </tr>
    </thead>
    <tbody>

        @foreach (var role in Model)
        {
            <tr>
                <td>@role.Name</td>
            </tr>
        }
    </tbody>
</table>
<a href="@Url.Action("CreateRole","Role")" class="btn btn-primary">Create new Role</a>